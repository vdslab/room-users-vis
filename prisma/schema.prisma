// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id     String   @id
  name   String?
  icon   String
  Access Access[]
}

model Access {
  user      User     @relation(fields: [user_id], references: [id])
  user_id   String
  check_in  DateTime
  check_out DateTime

  @@id([user_id, check_in])
}

